local Version = "1.6.41"
local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/download/" .. Version .. "/main.lua"))()

WindUI:SetNotificationLower(true)
WindUI:SetFont("rbxassetid://font-id-here")
WindUI:SetTheme("Dark")

local Window = WindUI:CreateWindow({
    Title = "荔枝脚本 | 免费",
    Icon = "door-open",
    Author = "荔枝 UI",
    Folder = "MyTestHub",
})

Window:Tag({
    Title = "v1.1.4",
    Color = Color3.fromHex("#30ff6a")
})

WindUI:Notify({
    Title = "荔枝脚本通知",
    Content = "荔枝脚本，禁止倒卖否则死浮木😡😡😡",
    Duration = 3, -- 3 seconds
    Icon = "bird",
})



WindUI:AddTheme({
    Name = "Dark",
    Accent = "#18181b",
    Dialog = "#18181b",
    Outline = "#FFFFFF",
    Text = "#FFFFFF",
    Placeholder = "#999999",
    Background = "#0e0e10",
    Button = "#52525b",
    Icon = "#a1a1aa",
})


local Tab = Window:Tab({
    Title = "通知",
    Icon = "81985649222682",
    Locked = false,
})


local Tab = Window:Tab({
    Title = "通用",
    Icon = "",
    Locked = false,
})

local Button = Tab:Button({
    Title = "飞行v3",
    Desc = "飞行",
    Locked = false,
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/mciklw/mciklwscript/refs/heads/main/flyvthree"))()
    end
})

local Tab = Window:Tab({
    Title = "俄亥俄州",
    Icon = "",
    Locked = false,
})

local Button = Tab:Button({
    Title = "0hio",
    Desc = "不需要卡密",
    Locked = false,
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/gycgchgyfytdttr/shenqin/refs/heads/main/%E7%89%9B%E9%80%BC%E7%89%9B%E7%9C%9F%E7%89%9B%E9%80%BC.lua"))()
    end
})

local Tab = Window:Tab({
    Title = "自然灾害",
    Icon = "",
    Locked = false,
})

local Button = Tab:Button({
    Title = "自然灾害",
    Desc = "不需要卡密",
    Locked = false,
    Callback = function()
        print("clicked")
    end
})

local Tab = Window:Tab({
    Title = "设置",
    Icon = "Tab",
    Locked = false,
})

Window:SelectTab(2) -- Number of Tab
