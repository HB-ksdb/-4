local Library = loadstring(game:HttpGet("https://pastebin.com/raw/Q5DRf1AG",true))()

local Forums = Library.new('DarkTheme')

local preview = Forums:NewSection("页面")
local preview1 = Forums:NewSection("页面2")

preview:NewButton("按钮", function()

end)
preview:NewToggle("开关", function(state)
print("Toggle: "..state)
end)
preview:NewTextBox("输入框", function(text)
print(text)
end)
preview:NewSlider("滑块", 0, 400, function(value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)
local list = {
    "Option 1",
    "Option 2",
    "Option 3",
}
preview:NewDropdown("列表", list, function(item)
   
end)

preview1:Seperator()

local btn = preview1:NewButton("按钮", function()
    print("Button")
end)
preview1:NewButton("更新按钮内容", function()
    btn:Update("更新")
end)

preview1:Seperator()
getgenv().stt = false
local toggle = preview1:NewToggle("开关: ", function(status)
    getgenv().stt = status
end)
coroutine.wrap(function()
    while wait() do
        toggle:Update("开关: "..tostring(getgenv().stt))
    end
end)()
preview1:Seperator()

getgenv().newTxt = "等待输入"
local box = preview1:NewTextBox("等待输入", function(newText)
    getgenv().newTxt = newText
end)
coroutine.wrap(function()
    while wait() do
        box:Update(tostring(getgenv().newTxt))
    end
end)()

preview1:Seperator()

local drop = preview1:NewDropdown("玩家列表", {}, function(item)
   print(item)
end)
local plrTable = {}
preview1:NewButton("刷新玩家列表", function()
    for i,v in pairs(game.Players:GetChildren()) do
        if v ~= game.Players.LocalPlayer then
            table.insert(plrTable, v.Name)
        end
    end
    drop:Refresh(plrTable)
    plrTable = {}
end)

preview1:Seperator()
