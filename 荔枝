    Icon = "door-open",
    Author = "荔枝 UI",
    Folder = "MyTestHub",
})

WindUI:Popup({
    Title = "荔枝脚本欢迎你",
    Icon = "info",
    Content = "荔枝脚本Q群746820543",
    Buttons = {
        {
            Title = "关闭",
            Callback = function() end,
            Variant = "Tertiary",
        },
        {
            Title = "启动",
            Icon = "arrow-right",
            Callback = function() end,
            Variant = "Primary",
        }
    }
})

Window:Tag({
    Title = "v1.1.4",
    Color = Color3.fromHex("#30ff6a")
})

WindUI:Notify({
    Title = "荔枝脚本通知",
