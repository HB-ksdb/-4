local Version = "1.6.41"
local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/download/" .. Version .. "/main.lua"))()

local Window = WindUI:CreateWindow({
    Title = "荔枝脚本 | 免费",
    Icon = "door-open",
    Author = "荔枝 UI",
    Folder = "MyTestHub",
})

Window:Tag({
    Title = "v1.1.4",
    Color = Color3.fromHex("#30ff6a")
})

WindUI:Notify({
    Title = "荔枝脚本通知",
    Content = "荔枝脚本，禁止倒卖否则死浮木😡😡😡",
    Duration = 3, -- 10 seconds
    Icon = "bird",
})


local Tab = Window:Tab({
    Title = "通知",
    Icon = "",
    Locked = false,
})


local Tab = Window:Tab({
    Title = "通用",
    Icon = "",
    Locked = false,
})

local Button = Tab:Button({
    Title = "飞行v3",
    Desc = "飞行",
    Locked = false,
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/mciklw/mciklwscript/refs/heads/main/flyvthree"))()
    end
})

Window:SelectTab(2) -- Number of Tab

local Section = Window:Section({
    Title = "脚本↓↓↓",
    Icon = "bird",
    Opened = true,
})

local Tab = Window:Tab({
    Title = "被遗弃",
    Icon = "bird",
    Locked = false,
})

local Button = Tab:Button({
    Title = "被遗弃XK",
    Desc = "无卡密中文",
    Locked = false,
    Callback = function()
        loadstring(game:HttpGet(('https://github.com/devslopo/DVES/raw/main/XK%20Hub')))()
    end
})

Window:SelectTab(2) -- Number of Tab

local Tab = Window:Tab({
    Title = "战争大亨",
    Icon = "bird",
    Locked = false,
})

local Button = Tab:Button({
    Title = "战争大亨XK",
    Desc = "无卡密中文",
    Locked = false,
    Callback = function()
        loadstring(game:HttpGet(('https://github.com/devslopo/DVES/raw/main/XK%20Hub')))()
    end
})

local Tab = Window:Tab({
    Title = "最强战场",
    Icon = "bird",
    Locked = false,
})

local Button = Tab:Button({
    Title = "最强战场无限侧闪",
    Desc = "无卡密英文",
    Locked = false,
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/10tempest01/tempest-hub/refs/heads/main/Launcher.lua"))()
